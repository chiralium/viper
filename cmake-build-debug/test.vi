function string_info(string) : {
    if (string[F->i] == "\n") : {
        F->words_count = F->words_count + 1;
        return 0;
    } else : {
        if (string[F->i] == " ") : {
            F->words[] = F->buffer;
            F->buffer = "";
            F->words_count = F->words_count + 1;
        } else : {
            F->buffer = F->buffer + string[F->i];
        }
        F->i = F->i + 1;
        string_info(string);
    }
}

