namespace Animal : {
    weight = 160;
    height = 50;
    name = "Animal";

    function do() : {
        output(__name__ + " are do something!\n");
    }
}

namespace Dog < Animal : {
    function do() : {
        output(__name__ + " are barking!\n");
    }

    function parent_do() : {
        do();
    }

    function is_it() : {
        output("is it is an " + name + "\n");
    }

    function get_height() : {
        output(string(height) + "\n");
    }
    name = "not-an-animal";
}

Dog->is_it();
Dog->do();
Dog->get_height();
output(Animal->name + "\n");
Dog->parent_do();