namespace A : {
    "Custom namespace for function";
    pi = 3.14; g = 9.8; e = 2.7;
};

function A foo() : {
    function f(x) : {
        return x;
    }

    function f(x, y) : {
        return 1 + (x / y);
    }

    function f(x, y, z) : {
        return x ^ z + 2 * x * y - y ^ z;
    }
    f1 = f(pi); f2 = f(f1, g); f3 = f(f1, f2, e);
    return f1 + f2 + f3;
}

foo();