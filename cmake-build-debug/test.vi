tree = {"key" => 0, "value" => 0, "left" => NONE, "right" => NONE};

function insert(root, node) : {
    if (root <=> NONE) : {
        return node;
    } else : {
        if (root["key"] > node["key"]) : {
            if (root["left"] <=> NONE) : {
                root["left"] = node;
            } else : {
                insert(root["left"], node);
            }
        } else (root["key"] < node["key"]) : {
            if (root["right"] <=> NONE) : {
                root["right"] = node;
            } else : {
                insert(root["right"], node);
            }
        } else (root["key"] == node["key"]) : {
            root["value"] = node["value"];
        }
    }
}

