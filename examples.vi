};

m = 4; n = 3;

i = 0;
positive = 0; negative = 0; zero = 0;
while (i < m) : {
    j = 0;
    while (j < n) : {
        if (M[i][j] > 0) : {
            positive = positive + 1;
        } else (M[i][j] < 0) : {
            negative = negative + 1;
        } else : {
            zero = zero + 1;
        }
        j = j + 1;
    }
    i = i + 1;
}

