fibo = {};
function fib(n) : {
    if (n == 0) : {
        return n;
    } else : {
        f = n + fib(n - 1);
        fibo[] = f;
        return f;
    }
}
fib(10);
fibo;

tree = {"key" => 0, "value" => 0, "left" => NONE, "right" => NONE};

function insert(root, node) : {
    if (root <=> NONE) : {
        return node;
    } else : {
        if (root["key"] > node["key"]) : {
            if (root["left"] <=> NONE) : {
                root["left"] = node;
            } else : {
                insert(root["left"], node);
            }
        } else (root["key"] < node["key"]) : {
            if (root["right"] <=> NONE) : {
                root["right"] = node;
            } else : {
                insert(root["right"], node);
            }
        } else (root["key"] == node["key"]) : {
            root["value"] = node["value"];
        }
    }
}

function find(root, key) : {
    if (root <=> NONE) : {
        return NONE;
    } else : {
        if (root["key"] == key) : {
            return root["value"];
        } else (root["key"] > key) : {
            return find(root["left"], key);
        } else (root["key"] < key) : {
            return find(root["right"], key);
        }
    }
}

