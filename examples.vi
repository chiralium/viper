namespace arithmetica : {
    function add(x, y) : {
        return x + y;
    }

    function sub(x, y) : {
        return x - y;
    }
}

OP = {"+" => arithmetica->add, "-" => arithmetica->sub};

F->i = 0;
function calc() : {
    if (WORDS[F->i] == "END") : {
        return 0;
    } else (WORDS[F->i] == "+")  : {
        return OP[WORDS[F->i]](WORDS[F->i - 1], " " + WORDS[F->i + 1]);
    } else : {
        F->i = F->i + 1;
        return calc();
    }
}
